<!-- Background image -->
    <div id="wrapper">

     
     

        <!-- ****** New Arrivals Area Start ****** -->
        <section class="new_arrivals_area section_padding_100_0 clearfix">
          <div class="container">
              <div class="row" style="justify-content: center;">
                  <div class="col-12">
                      <div class="section_heading text-center">
                        <h2 >Produ<span style="background-color: #d7e420;"> ct</span></h2> 
                      </div>
                      <div class="col-8">
                          <form action="/search">
                              <input type="search" name="q" value="" id=""style="border-radius:2rem">
                              <input type="submit" value="search"style="border-radius:2rem" >
                          </form>
                          
                      </div>
                  </div>
                  
              </div>
          </div>

          <div class="karl-projects-menu mb-100">
              <div class="text-center portfolio-menu">
                  <button class="btn active" data-filter="*">ALL</button>
                  <% if (categories != null) { %>
                      <% for(let category of categories){ %>
                          <button class="btn" data-filter=".<%= category %>" style ="background-color: #d7e420; border:1rem solid #d7dbab; border-radius: 2rem 3rem;"><%= category %></button>
                      <% } %>
                  <% } %>
                  
              </div>
          </div>

          <div class="container">
              <div class="row karl-new-arrivals">
                  <% for( let i = 0; i < products.length; i++ ) { %>
                      <!-- Single gallery Item Start -->
                      <div class="col-12 col-sm-6 col-md-4 single_gallery_item <%= products[i].category %> wow fadeInUpBig" data-wow-delay="0.3s">
                          <!-- Product Image -->
                          <div class="product-img">
                              <img src="<%= products[i].thumbnail %>" alt="product img">
                              <div class="product-quicview">
                                  <!-- data-toggle="modal" data-target="#quickview" -->
                                  <a href="/<%= products[i].id %>"><i class="ti-plus" style=" color:#d7e420" onclick="pass('<%= products[i].id %>')"></i></a>
                              </div>
                          </div>
                          <!-- Product Description -->
                          <div class="product-description">
                              <h4 class="product-price">$<%= products[i].price %></h4>
                              <p><%= products[i].description %> </p>
                              <!-- Add to Cart -->
                              <a href="#" class="add-to-cart-btn">ADD TO CART</a>
                          </div>
                      </div>
                  <% } %>
              </div>
          </div>
      </section>
      <!-- ****** New Arrivals Area End ****** -->

      <!-- ****** Offer Area Start ****** -->
      <div class="lightbox">
        <div class="row">
          <div class="col-lg-6">
           
            <img
              src="img/intro/4.jpg"
              data-mdb-img="https://mdbcdn.b-cdn.net/img/Photos/Square/1.webp"
